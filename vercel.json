{
  "redirects": [
    {
      "source": "/handymangreeleyco.com/(.*)",
      "destination": "https://www.handymangreeleyco.com/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/blog",
      "destination": "/index.html"
    },
    {
      "source": "/blog/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/services",
      "destination": "/index.html"
    },
    {
      "source": "/about",
      "destination": "/index.html"
    },
    {
      "source": "/terms",
      "destination": "/index.html"
    },
    {
      "source": "/light-fixtures",
      "destination": "/index.html"
    },
    {
      "source": "/((?!api/|_next/|_static/|favicon.ico|robots.txt|sitemap.xml|.*\\.).*)$",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}